{
    "version": 2,
    "name": "videos-api",
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/$1"
        }
    ]
}